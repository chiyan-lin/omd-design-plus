{
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "workspaces": [
    "packages/*",
    "develpo",
    "docs"
  ],
  "scripts": {
    "cz": "czg",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepare:e2e": "if [ ! -d \"docs/.vitepress/dist\" ]; then pnpm run docs:build; fi;",
    "dev": "pnpm -C devolop dev",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "tsx scripts/gen-version.ts",
    "diff:table": "tsx scripts/build-table.ts",
    "update:version": "tsx scripts/update-version.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C hammer/build start",
    "build:theme": "pnpm run -C packages/style build",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "lint:commit": "commitlint --from $(git merge-base origin/dev HEAD) --to HEAD > ./commit-lint.txt",
    "typecheck": "run-p typecheck:web typecheck:devolop typecheck:node typecheck:vite-config typecheck:vitest",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "typecheck:devolop": "vue-tsc -p tsconfig.devolop.json --composite false --noEmit",
    "typecheck:vite-config": "vue-tsc -p tsconfig.vite-config.json --composite false --noEmit",
    "typecheck:vitest": "vue-tsc -p tsconfig.vitest.json --composite false --noEmit",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "docs:gen-locale": "pnpm run -C docs gen-locale",
    "docs:crowdin-credentials": "pnpm run -C docs crowdin-credentials",
    "stub": "pnpm run -r --parallel stub",
    "prepare": "husky install",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\""
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.10",
    "@omd-design-plus/components": "workspace:*",
    "@omd-design-plus/constants": "workspace:*",
    "@omd-design-plus/directives": "workspace:*",
    "@omd-design-plus/hooks": "workspace:*",
    "@omd-design-plus/test-utils": "workspace:*",
    "@omd-design-plus/utils": "workspace:*",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^9.6.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.7",
    "escape-html": "^1.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@esbuild-kit/cjs-loader": "^2.4.1",
    "@omd-design-plus/build": "workspace:^0.0.1",
    "@omd-design-plus/build-utils": "workspace:^0.0.1",
    "@omd-design-plus/eslint-config": "workspace:0.0.1",
    "@pnpm/find-workspace-packages": "^4.0.43",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^8.9.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.10",
    "@types/jsdom": "^16.2.15",
    "@types/node": "*",
    "@types/sass": "^1.43.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/ui": "0.16.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "chalk": "^5.2.0",
    "concurrently": "^7.6.0",
    "consola": "^2.15.3",
    "csstype": "^2.6.21",
    "cz-git": "^1.4.1",
    "czg": "^1.4.1",
    "eslint": "^8.29.0",
    "eslint-define-config": "^1.12.0",
    "expect-type": "^0.13.0",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.2",
    "jsdom": "16.4.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "puppeteer": "^17.1.3",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.56.2",
    "ts-morph": "^14.0.0",
    "tsx": "^3.12.1",
    "type-fest": "^2.19.0",
    "typescript": "^4.9.4",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-macros": "^0.11.2",
    "vitest": "0.12.6",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^0.38.9"
  },
  "engines": {
    "node": ">= 16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
