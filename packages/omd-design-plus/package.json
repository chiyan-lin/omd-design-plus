{
  "name": "omd-design-plus",
  "version": "0.0.0-dev.1",
  "description": "A Component Library for Vue 3 in OMD",
  "keywords": [
    "omd-design-plus",
    "component library",
    "vue"
  ],
  "homepage": "https://chiyan-lin.github.io/omd-design-plus/docs/",
  "bugs": {
    "url": "https://github.com/chiyan-lin/omd-design-plus/issues"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs",
      "types": "./es/index.d.ts"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*": "./es/*.mjs",
    "./lib/*.js": "./lib/*.js",
    "./lib/*": "./lib/*.js",
    "./*": "./*"
  },
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiyan-lin/omd-design-plus.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "style": "dist/index.css",
  "sideEffects": [
    "dist/*",
    "style/**/*.css",
    "style/src/**/*.scss",
    "es/components/*/style/*",
    "lib/components/*/style/*"
  ],
  "peerDependencies": {
    "ant-design-vue": ">=3.2.13",
    "lodash": ">=4.17.21",
    "vue": ">=3.2.37"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.5.0",
    "@floating-ui/dom": "^1.0.10",
    "@popperjs/core": "npm:@sxzz/popperjs-es@^2.11.7",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^9.6.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.7",
    "escape-html": "^1.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "*",
    "csstype": "^2.6.21",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "vetur": {
    "tags": "tags.json",
    "attributes": "attributes.json"
  },
  "web-types": "web-types.json",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
